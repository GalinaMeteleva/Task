package by.tc.task01.dao.impl;

import by.tc.task01.dao.ApplianceDAO;
import by.tc.task01.entity.Appliance;
import by.tc.task01.entity.criteria.Criteria;

import java.util.List;
import java.util.ArrayList;
import java.io.IOException;
import java.lang.Exception;

public class ApplianceDAOImpl implements ApplianceDAO {
	
	Criteria criteria;
	
	public String checkData(List<String>applianceData) throws DAOException {
		
		try {	
			ReadAppliances rd2 = new ReadAppliances();
			ApplianceDAOImpl impl = rd2.readingAppliance();
		}
		catch(FileNotFoundException e) {
			throw new DAOException("File not found");
		}
		catch(IOException e) {
			throw new DAOException("IOException");
		}
	}
	@Override
	public Appliance find(Criteria criteria) {
		
		ReadAppliances rd1 = new ReadAppliances();
		List<String> appl = rd1.getApplianceData();
		
		return (Appliance)appl;
	}
}
	
